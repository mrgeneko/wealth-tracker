{
  "name": "wealth-tracker-scrapers",
  "version": "1.0.0",
  "description": "web scraping via Chrome automation",
  "main": "scrapers/scrape_security_data.js",
  "scripts": {
    "start": "node scrapers/scrape_security_data.js",
    "migrate": "node scripts/migrate_assets_to_mysql.js",
    "check-normalized": "node scripts/check_scrapers_normalized_key.js",
    "test": "jest",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "node scripts/ci/run_tests_with_junit.js --testsdir=tests/integration",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:migration:pre": "jest tests/integration/migration/pre-migration-validation.test.js",
    "test:migration:execute": "jest tests/integration/migration/migration-execution.test.js",
    "test:migration:post": "jest tests/integration/migration/post-migration-validation.test.js",
    "test:migration:performance": "jest tests/integration/migration/performance-validation.test.js",
    "test:migration:all": "jest tests/integration/migration/ --testTimeout=120000",
    "test:backend:dashboard": "jest tests/unit/dashboard-ticker-api.test.js",
    "test:backend:api": "jest tests/unit/ticker-api-endpoints.test.js",
    "test:backend:compatibility": "jest tests/unit/backward-compatibility-symbol.test.js",
    "test:backend:all": "jest tests/unit/dashboard-ticker-api.test.js tests/unit/ticker-api-endpoints.test.js tests/unit/backward-compatibility-symbol.test.js",
    "test:frontend:utilities": "jest tests/unit/frontend-ticker-utilities.test.js",
    "test:frontend:forms": "jest tests/unit/add-position-form.test.js",
    "test:frontend:all": "jest tests/unit/frontend-ticker-utilities.test.js tests/unit/add-position-form.test.js",
    "test:e2e:ticker": "jest tests/integration/ticker-standardization-e2e.test.js",
    "test:all": "jest tests/ --testTimeout=120000",
    "test:coverage:comprehensive": "jest --coverage --testTimeout=120000",
    "migrate:execute": "node scripts/migrations/execute-migration.js",
    "migrate:execute:dry-run": "node scripts/migrations/execute-migration.js --dry-run",
    "migrate:rollback": "node scripts/migrations/execute-migration.js --rollback"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.4",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "kafkajs": "^2.2.4",
    "luxon": "^3.7.2",
    "mysql2": "^3.15.3",
    "puppeteer": "^24.15.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "socket.io": "^4.8.1",
    "ws": "^8.18.3",
    "yahoo-finance2": "^3.10.2"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  }
}
